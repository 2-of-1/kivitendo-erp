<=&lt;
>=&gt;
\n=<br>

[Template/XML]
order=< > \n
<=&lt;
>=&gt;
\n=<br>

[Template/LaTeX]
order=\\ <pagebreak> & \n \r " $ <bullet> % _ # ^ { } < > £ ± ² ³ ° § ® © \xad \xa0 ➔ → ← | −
\\=\\textbackslash\s
<pagebreak>=
"=''
&=\\&
$=\\$
<bullet>=$\\bullet$
%=\\%
_=\\_
# A hash mark starts a comment; therefore the line is ignored. So use
# its hex code instead.
\x23=\\#
{=\\{
}=\\}
<=$<$
>=$>$
£=\\pounds\s
\n=\\newline\s
\r=
±=$\\pm$
^=\\^\\\s
²=$^2$
³=$^3$
°=$^\\circ$
§=\\S\s
®=\\textregistered\s
©=\\textcopyright\s
\xad=\\-
➔=$\\rightarrow$
→=$\\rightarrow$
←=$\\leftarrow$
\xa0=~
|=\\textbar\s
−={\\textemdash}

